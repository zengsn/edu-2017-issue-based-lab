    "patch" : "@@ -0,0 +1,74 @@\n+<%@ page language=\"java\" contentType=\"text/html;charset=utf-8\"%>\n+<%@ page import=\"java.util.*,java.io.*\" %>\n+<html>\n+<!-- 新 Bootstrap 核心 CSS 文件 -->\n+<link rel=\"stylesheet\" href=\"http://cdn.bootcss.com/bootstrap/3.3.0/css/bootstrap.min.css\">\n+\n+<!-- 可选的Bootstrap主题文件（一般不用引入） -->\n+<link rel=\"stylesheet\" href=\"http://cdn.bootcss.com/bootstrap/3.3.0/css/bootstrap-theme.min.css\">\n+\n+<!-- jQuery文件。务必在bootstrap.min.js 之前引入 -->\n+<script src=\"http://cdn.bootcss.com/jquery/1.11.1/jquery.min.js\"></script>\n+\n+<!-- 最新的 Bootstrap 核心 JavaScript 文件 -->\n+<script src=\"http://cdn.bootcss.com/bootstrap/3.3.0/js/bootstrap.min.js\"></script>\n+<head><title>学生信息登记表</title></head>\n+<body>\n+<!--对提交到该JSP页面含有中文的信息实现以中文正常显示-->\n+<%request.setCharacterEncoding(\"utf-8\");%>\n+<center>\n+<div class=\"container\">\n+<table class=\"table\">\n+<tbody>\n+<tr>\n+<td><b>注册名称</b></td>\n+<!--从用户提交表单获取注册名称-->\n+<td><%=request.getParameter(\"registername\")%></td>\n+</tr>\n+<tr>\n+<td><b>登陆密码</b></td>\n+<!--从用户提交表单获取登录密码-->\n+<td><%=request.getParameter(\"pwd\")%></td>\n+</tr>\n+<tr>\n+<td><b>真实姓名</b></td>\n+<!--从用户提交表单获取真是姓名-->\n+<td><%=request.getParameter(\"TrueName\")%></td>\n+</tr>\n+<tr>\n+<td><b>身份证号</b></td>\n+<!--从用户提交表单获取身份证号-->\n+<td><%=request.getParameter(\"PersonalCode\")%></td>\n+</tr>\n+<tr>\n+<td><b>性别</b></td>\n+<!--从用户提交表单获取性别-->\n+<td>\n+<%=request.getParameter(\"Sex\")%>\n+</td>\n+</tr>\n+<tr>\n+<td><b>出生日期</b></td>\n+<!--从用户提交表单获取出生日期-->\n+<td><%=request.getParameter(\"BirthDate\")%></td>\n+</tr>\n+<tr>\n+<td><b>联系电话</b></td>\n+<!--从用户提交表单获取联系电话-->\n+<td><%=request.getParameter(\"HomeNum\")%></td>\n+</tr>\n+<tr>\n+<td><b></b>E-mail</td>\n+<td><%=request.getParameter(\"Email\")%></td>\n+</tr>\n+<tr>\n+<td><b>系别</b></td>\n+<!--从用户提交表单获取系别-->\n+<td><%=request.getParameter(\"Department\")%></td>\n+</tr>\n+</tbody>\n+</table>\n+</div>\n+</center>\n+</body>\n+</html>\n\\ No newline at end of file"
